# Description:  sysvinit yonetimi
# URL:          http://sysv-rc-conf.sourceforge.net/
# Maintainer:   milisarge
# Packager :	milisarge
# Depends on:   perl-curses perl-curses-ui

name=sysv-rc-conf
version=0.98
release=1
source=($name.pl)

build() {
	mkdir $PKG/usr
	mkdir $PKG/usr/bin
    install -m755 $name.pl $PKG/usr/bin/$name
}
