# Tanım:	ISO 9660 image manipulasyon araci
# URL:	https://www.gnu.org/software/xorriso/
# Paketçi: milisarge
# Gerekler: 

name=xorriso
version=1.4.4
release=1
source=(https://www.gnu.org/software/$name/$name-$version.tar.gz)

build() {
  cd $name-$version

  ./configure --prefix=/usr

  make all
  make DESTDIR=$PKG install
}
