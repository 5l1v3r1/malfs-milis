# Tanım: X sahiplik dosyasi
# URL: http://xorg.freedesktop.org
# Paketçi: milisarge
# Gerekler: xorg-server

name=xorg-xauth
version=1.0.9
release=2
source=(http://xorg.freedesktop.org/releases/individual/app/xauth-$version.tar.bz2)

derle() {
	cd xauth-$version

	./configure --prefix=/usr

	make
	make DESTDIR=$PKG install
}
