# Description: A real-time software synthesizer.
# URL: http://www.fluidsynth.org/
# Packager: milisarge
# Depends on:

name=fluidsynth
version=1.1.3
release=1
source=(http://download.sourceforge.net/$name/$name-$version.tar.gz)

build() {
  cd $name-$version
  ./configure --prefix=/usr \
              --mandir=/usr/man \
              --disable-debug \
  make
  make DESTDIR=$PKG install
}
