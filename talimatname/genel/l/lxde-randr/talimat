name=lxde-randr
_name=lxrandr
version=0.3.0
release=1
source=(http://downloads.sourceforge.net/lxde/lxrandr-$version.tar.xz)

derle() {
    cd ${_name}-$version
    ./configure --prefix=/usr
    make
    make DESTDIR=$PKG install

}

  
