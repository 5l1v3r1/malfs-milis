# Gerekler: dbus-glib gtk2 libunique1 lsb-release polkit vala upower pm-utils

name=lxde-session
_name=lxsession
version=0.5.2
release=3

source=(http://downloads.sourceforge.net/lxde/${_name}-$version.tar.xz)


derle() {
cd ${_name}-$version
./configure --prefix=/usr --disable-man
make
make DESTDIR=$PKG install
}
