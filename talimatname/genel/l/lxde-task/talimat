name=lxde-task
_name=lxtask
version=0.1.6
release=1
source=(http://downloads.sourceforge.net/lxde/${_name}-$version.tar.xz)

derle() {
    cd ${_name}-$version
    ./configure --prefix=/usr
    make
    make DESTDIR=$PKG install

}
