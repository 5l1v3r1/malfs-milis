name=lxde-terminal
_name=lxterminal
version=0.2.0
release=1

source=(http://downloads.sourceforge.net/lxde/lxterminal-$version.tar.gz)

derle() {
cd ${_name}-$version
./configure --prefix=/usr \
make
make DESTDIR=$PKG install
}
  
