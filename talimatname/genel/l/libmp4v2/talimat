# Tanım: MPEG-4 kitaplığı
# URL: http://code.google.com/p/mp4v2
# Paketçi: milisarge
# Gerekler:

name=libmp4v2
version=2.0.0
release=1

source=(https://sources.archlinux.org/other/packages/$name/mp4v2-${version}.tar.bz2)

build() {
	cd mp4v2-$version
	./configure --prefix=/usr
	make
	make DESTDIR=$PKG install
}
