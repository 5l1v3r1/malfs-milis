name=libgee06
_name=libgee
version=0.6.8
release=1

source=(http://ftp.acc.umu.se/pub/gnome/sources/$_name/${version%.*}/$_name-$version.tar.xz)
	
derle() {
cd $_name-$version
./configure --prefix=/usr
make 
make DESTDIR=$PKG install
}
