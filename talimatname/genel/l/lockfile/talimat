# Tanım: Platformdan bağımsız dosya kilitleme modülü
# URL: http://launchpad.net/pylockfile
# Paketçi: milisarge
# Gerekler: python-setuptools

name=lockfile
version=0.12.2
release=1

source=(https://pypi.python.org/packages/source/l/lockfile/lockfile-0.12.2.tar.gz)


derle() {
  cd "${SRC}/${name}-${version}"
  python setup.py install --root=$PKG
}
