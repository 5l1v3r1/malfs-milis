name=python-six
version=1.7.3
release=1

source=(
http://pypi.python.org/packages/source/s/six/six-$version.tar.gz)

build() {
    cd six-$version
    python2 setup.py check
    python2 setup.py install --prefix=/usr --root=$PKG
}
