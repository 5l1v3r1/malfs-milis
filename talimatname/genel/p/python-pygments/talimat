isim=python-pygments
surum=1.6
devir=1

kaynak=(
http://pypi.python.org/packages/source/P/Pygments/Pygments-$surum.tar.gz)

derle() {
    cd Pygments-$surum
    python2 setup.py install --prefix=/usr --root=$PKG
}
