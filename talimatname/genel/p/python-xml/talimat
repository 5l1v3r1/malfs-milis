isim=python-xml
surum=0.8.4
devir=1

kaynak=(http://downloads.sourceforge.net/pyxml/PyXML-0.8.4.tar.gz)

derle() {
	cd PyXML-$surum
	python2 setup.py build
	python2 setup.py install --prefix=/usr --root=$PKG

}

  
