isim=python-psutil
surum=2.1.3
devir=1

kaynak=(
https://pypi.python.org/packages/source/p/psutil/psutil-$surum.tar.gz)

derle() {
    cd psutil-$surum
    python2 setup.py install --prefix=/usr --root=$PKG
}

