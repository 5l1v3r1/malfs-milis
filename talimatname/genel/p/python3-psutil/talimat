name=python3-psutil
version=2.1.3
release=1

source=(
https://pypi.python.org/packages/source/p/psutil/psutil-$version.tar.gz)

derle() {
    cd psutil-$version
    python3 setup.py install --prefix=/usr --root=$PKG
}

