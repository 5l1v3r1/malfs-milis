# Gerekler: python 

isim=python-xdg
surum=0.25
devir=1

kaynak=(http://people.freedesktop.org/~takluyver/pyxdg-$surum.tar.gz)

derle() {
	cd pyxdg-$surum
	python2 setup.py install --prefix=/usr --root=$PKG
}
