# Tanım: Python3 uyumluluk programları.
# URL: http://pypi.python.org/pypi/six/
# Paketçi: milisarge
# Gerekler: python 

isim=python3-six
surum=1.7.3
devir=1

kaynak=(
http://pypi.python.org/packages/source/s/six/six-$surum.tar.gz)

derle() {
    cd six-$surum
    python3 setup.py check
    python3 setup.py install --prefix=/usr --root=$PKG
}

