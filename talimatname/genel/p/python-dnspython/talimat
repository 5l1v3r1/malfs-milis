# Tanım: Python için bir DNS araç seti.
# URL: http://www.dnspython.org/
# Paketçi: milisarge
# Gerekler: python

isim=python-dnspython
surum=1.11.1
devir=1

kaynak=(
http://www.dnspython.org/kits/$surum/dnspython-$surum.tar.gz)

derle() {
    cd dnspython-$surum
    python2 setup.py install --prefix=/usr --root=$PKG
}
