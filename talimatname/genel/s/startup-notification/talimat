# Tanım: Uygulama başlatmayı izleme ve görüntüleme
# URL: http://freedesktop.org/wiki/Software/startup-notification
# Paketçi: milisarge
# Gerekler: xorg-libx11 xcb-util 

name=startup-notification
version=0.12
release=1

source=(http://www.freedesktop.org/software/$name/releases/$name-$version.tar.gz)

derle() {
	cd $name-$version
	./configure --prefix=/usr \
		--infodir=/usr/share/info \
		--mandir=/usr/share/man
	make
	make DESTDIR=$PKG install
}
  
