# Tanım: Konfigürasyon dosyalarını okuyup ağaç şeklinde görüntülemeyi sağlayan bir araç.
# URL: http://augeas.net
# Paketçi: milisarge
# Gerekler: libxml2

name=augeas
version=1.4.0
release=1
source=(http://download.augeas.net/$name-$version.tar.gz)

derle() {

    cd $name-$version

    ./configure --prefix=/usr

    make
    make DESTDIR=$PKG install

}
