# Gerekler: 
# Grup: sistem

isim=autoconf-archive
surum=2017.03.21
devir=1
kaynak=(http://ftpmirror.gnu.org/$isim/$isim-$surum.tar.xz)

derle() {
cd $isim-$surum
  ./configure --prefix=/usr
  make
  make DESTDIR=$PKG install
}
