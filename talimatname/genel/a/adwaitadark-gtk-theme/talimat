# Tanım: Koyu bir gtk2 teması
# URL: https://www.xfce-look.org/p/1099669/
# Paketçi: yasarciv67@gmail.com
# Gerekler: 

isim=adwaitadark-gtk-theme
surum=1
devir=1
kaynak=(https://dl.opendesktop.org/api/files/download/id/1462546831/175933-AdwaitaDark.tar.gz)

derle() {
	cd $SRC/AdwaitaDark 	
	install -d "$PKG/usr/share/themes/AdwaitaDark/"
	cp -a * "$PKG/usr/share/themes/AdwaitaDark/"
}
