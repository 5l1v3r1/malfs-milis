name=asterisk-core-sounds-fr-alaw
version=1.4.27
release=1
source=(http://downloads.digium.com/pub/telephony/sounds/releases/$name-$version.tar.gz)

derle() {

    install -d -m 750 $PKG/var/lib/asterisk/sounds
    mv $SRC/* $PKG/var/lib/asterisk/sounds

}
