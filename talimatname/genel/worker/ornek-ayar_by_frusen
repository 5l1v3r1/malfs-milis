				changeprogress = false;
				changetext = false;
				progressuseoutput = false;
				wintextuseoutput = false;
			}
			ReloadOp {
				mode = current;
				resetdirsizes = false;
				keepfiletypes = false;
			}
		}
	}
	button {
		position = 258;
		title = "convert image";
		color = 1,0;
		shortkeys {
		}
		commands {
			OwnOp {
				separateeachentry = true;
				recursive = false;
				start = terminalwait;
				com = "convert {uf} {fE}.{Rs{Enter type as extension (without .):}{jpg}}";
				viewstr = "";
				inbackground = false;
				takedirs = false;
				dontcd = false;
			}
			ReloadOp {
				mode = current;
				resetdirsizes = false;
				keepfiletypes = false;
			}
		}
	}
