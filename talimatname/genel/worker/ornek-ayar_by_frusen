			OwnOp {
				separateeachentry = false;
				recursive = false;
				start = normal;
				com = "mv {pop 0} {top 5}{top 4}{pop 6}";
				viewstr = "";
				inbackground = false;
				takedirs = false;
				dontcd = false;
			}
			ScriptOp {
				type = push;
				pushuseoutput = true;
				dodebug = false;
				wpurecursive = false;
				wputakedirs = false;
				stacknr = 4;
