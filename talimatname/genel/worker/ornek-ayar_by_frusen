				type = if;
				pushuseoutput = false;
				dodebug = false;
				wpurecursive = false;
				wputakedirs = false;
				stacknr = 0;
				iftest = "?{test -e \"{p}/{top 5}{top 4}{top 6}\"} != 0";
				iflabel = "rename";
				wintype = leave;
				changeprogress = false;
				changetext = false;
				progressuseoutput = false;
				wintextuseoutput = false;
			}
