				wintype = leave;
				changeprogress = true;
				changetext = false;
				progressuseoutput = true;
				wintextuseoutput = true;
				progress = "expr {top 2} \\\\* 100 / {top 3}";
			}
			ScriptOp {
				type = push;
				pushuseoutput = true;
				dodebug = false;
				wpurecursive = false;
				wputakedirs = false;
				stacknr = 6;
				pushstring = "echo {top 0} | sed -n s/'.*\\\\.\\\\([^\\\\.]*$\\\\)'/'\\\\.\\\\1'/p";
				wintype = leave;
				changeprogress = false;
				changetext = false;
				progressuseoutput = false;
				wintextuseoutput = false;
			}
			ScriptOp {
				type = label;
				pushuseoutput = false;
				dodebug = false;
				wpurecursive = false;
				wputakedirs = false;
				stacknr = 0;
				label = "findnext";
				wintype = leave;
				changeprogress = false;
				changetext = false;
				progressuseoutput = false;
				wintextuseoutput = false;
			}
