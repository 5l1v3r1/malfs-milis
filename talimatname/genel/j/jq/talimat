name=jq
version=1.5
release=1
source=(https://github.com/stedolan/$name/releases/download/$name-$version/$name-$version.tar.gz)

derle() {
	cd $name-$version

	./configure --prefix=/usr
	make
	make DESTDIR=$PKG install
}
