# Description: Caja i√ßin Python
# URL: http://matsusoft.com.ar/projects/mate
# Packager: yasarciv
# Depends on: caja python-gobject2

name=python-caja
version=1.16.0
release=1
source=(http://pub.mate-desktop.org/releases/${version%.*}/$name-$version.tar.xz)

build() {
cd $name-$version
./configure \
        --prefix=/usr \
        --disable-static
make
make DESTDIR=$PKG install
}
