# Description: enlightenment widget set
# URL: http://www.enlightenment.org/
# Packager: milisarge
# Depends on: efl

name=elementary
version=1.17.1
release=1
source=(http://download.enlightenment.org/rel/libs/elementary/elementary-$version.tar.xz)

build()
{
    cd elementary-$version 
    ./configure --prefix=/usr
    make
    make DESTDIR=$PKG install
}
