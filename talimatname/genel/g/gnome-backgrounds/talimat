name=gnome-backgrounds
version=3.22.1
release=1

source=(http://ftp.gnome.org/pub/gnome/sources/$name/${version:0:4}/$name-$version.tar.xz)

derle() {
cd $name-$version
./configure --prefix=/usr 
make
make DESTDIR=$PKG install
}
