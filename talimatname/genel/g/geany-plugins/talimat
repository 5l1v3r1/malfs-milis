# Tanım: Geany için birçok eklenti
# URL: http://plugins.geany.org/
# Paketçi: milisarge
# Gerekler: geany

name=geany-plugins
version=1.26
release=1
source=(http://plugins.geany.org/geany-plugins/$name-$version.tar.bz2)

build() {
cd $name-$version
./configure --prefix=/usr
make
make DESTDIR=$PKG install
}
