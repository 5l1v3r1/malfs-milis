# Tanım: GTK-engines paketi, sekiz tema / motor ve GTK2 için iki ilave motor içerir.
# URL: http://ftp.gnome.org/pub/gnome/sources/gtk-engines
# Paketçi: milisarge
# Gerekler: gtk2 lua 

name=gtk-engines
version=2.20.2	
release=1
source=(http://ftp.gnome.org/pub/gnome/sources/gtk-engines/2.20/$name-$version.tar.bz2)
 
build() {

	cd $name-$version
	
	./configure --prefix=/usr 
	
	make

	make DESTDIR=$PKG install

}
