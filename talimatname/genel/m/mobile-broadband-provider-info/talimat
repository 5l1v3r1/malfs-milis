# Paketçi: alihan-ozturk28@hotmail.com
# Gerekler: 

name=mobile-broadband-provider-info
version=20151214
release=1

source=(http://download.gnome.org/sources/$name/$version/$name-$version.tar.xz)

derle() {
  cd $name-$version
  ./configure --prefix=/usr
  make
  make DESTDIR=$PKG install
}
