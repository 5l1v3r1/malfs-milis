# Tanım: MATE temaları
# URL: http://mate-desktop.org
# Paketçi: milisarge yasarciv
# Gerekler: gtk2 intltool gtk-engines gtk-engine-murrine mate-icon-theme


name=mate-themes
version=3.22.12
release=1
source=(http://pub.mate-desktop.org/releases/themes/${version%.*}/${name}-${version}.tar.xz)

derle() {
	cd ${name}-${version}
    ./configure \
        --prefix=/usr 
make
make DESTDIR=$PKG install
}
