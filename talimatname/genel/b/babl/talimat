isim=babl
surum=0.1.14
devir=1
kaynak=(http://download.gimp.org/pub/$isim/0.1/$isim-$surum.tar.bz2)

derle() {
	cd $isim-$surum
	./configure --prefix=/usr --disable-docs
	make
	make DESTDIR=$PKG install
}
