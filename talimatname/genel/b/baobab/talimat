isim=baobab
surum=3.22.1
devir=1

kaynak=(http://ftp.gnome.org/pub/gnome/sources/$isim/${surum%.*}/$isim-$surum.tar.xz)

derle() {
 cd $isim-$surum
 ./configure --prefix=/usr
 make
 make DESTDIR=$PKG install
}
