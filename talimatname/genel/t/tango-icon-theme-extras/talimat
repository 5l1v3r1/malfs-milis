# Tanım: Tango Masaüstü Projesi, sanatçıların ve tasarımcıların uyabileceği bir simge stili kılavuzunu tanımlar.
# URL: http://tango.freedesktop.org/
# Paketçi: milisarge
# Gerekler: icon-naming-utils imagemagick 

name=tango-icon-theme-extras
version=0.1.0
release=1

source=(http://tango.freedesktop.org/releases/$name-$version.tar.gz)

build()
{
cd $name-$version
./configure --prefix=/usr
make
make DESTDIR=$PKG install
}
