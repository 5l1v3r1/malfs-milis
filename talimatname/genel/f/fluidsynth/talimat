# Tanım: A real-time software synthesizer.
# URL: http://www.fluidsynth.org/
# Paketçi: milisarge
# Gerekler:

name=fluidsynth
version=1.1.3
release=1
source=(http://download.sourceforge.net/$name/$name-$version.tar.gz)

derle() {
  cd $name-$version
  ./configure --prefix=/usr \
              --mandir=/usr/man \
              --disable-debug 
  make
  make DESTDIR=$PKG install
}
