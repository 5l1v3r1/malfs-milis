name=four-in-a-row
version=3.22.1
release=1

source=(http://ftp.acc.umu.se/pub/gnome/sources/four-in-a-row/3.22/four-in-a-row-$version.tar.xz)

derle() {
cd $name-$version
	./configure --prefix=/usr     
	make
	make DESTDIR=$PKG install
}
