# Tanım: Http üzerinden tek yönlü senkronizasyon
# URL:         http://jw.tks6.net/files/crux/httpup_manual.html 
# Paketçi: milisarge
# Gerekler: 

name=httpup
version=0.4.0l
release=1
source=(http://crux.nu/files/$name-$version.tar.gz)

derle() {
    cd $name-$version
    make
    make DESTDIR=$PKG prefix=/usr mandir=/usr/share/man install
}
