# Tanım: CIFS dosya sistemi kullanıcı alanı araçları
# URL: http://www.samba.org
# Paketçi: milisarge
# Gerekler: samba

isim=cifs-utils
surum=6.5
devir=1
kaynak=( http://ftp.samba.org/pub/linux-cifs/$isim/$isim-$surum.tar.bz2)

derle() {
cd $isim-$surum
./configure --prefix=/usr --disable-systemd
make
make DESTDIR=$PKG install
}
