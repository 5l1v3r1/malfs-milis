name=cppunit
version=1.14.0
release=1
source=(http://dev-www.libreoffice.org/src/$name-$version.tar.gz)

derle() {
cd $name-$version
./configure --prefix=/usr --disable-static
make
make DESTDIR=$PKG install
}
