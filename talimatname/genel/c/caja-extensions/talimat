isim=caja-extensions
surum=1.18.1
devir=1
kaynak=(http://pub.mate-desktop.org/releases/${surum%.*}/$isim-$surum.tar.xz)

derle() {
  cd $isim-$surum
  ./configure \
        --prefix=/usr 
  make
  make DESTDIR=$PKG install
}
