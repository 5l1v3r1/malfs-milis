# Paketçi: milisarge
# Gerekler: 
# Grup: sistem

isim=check
surum=0.12.0
devir=1
kaynak=(https://github.com/libcheck/check/releases/download/0.12.0/check-0.12.0.tar.gz)

derle() {
cd $isim-$surum
./configure --prefix=/usr --disable-static
make
make DESTDIR=$PKG install
}
