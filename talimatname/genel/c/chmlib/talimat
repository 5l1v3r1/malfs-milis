isim=chmlib
surum=0.40
devir=1
kaynak=(http://morte.jedrea.com/~jedwin/projects/$isim/$isim-$surum.tar.bz2)

derle(){
    cd $isim-$surum
    ./configure --prefix=/usr \
                --enable-examples=yes
    make
    make DESTDIR=$PKG install
}
