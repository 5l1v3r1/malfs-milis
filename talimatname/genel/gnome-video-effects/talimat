name=gnome-video-effects
version=0.4.1
release=1

source=(http://ftp.gnome.org/pub/gnome/sources/$name/${version%.*}/$name-$version.tar.xz)

build() {
cd $name-$version
./configure --prefix=/usr 
make
make DESTDIR=$PKG install
}
