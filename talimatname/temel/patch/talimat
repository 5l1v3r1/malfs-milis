# Tanım: The GNU patch tool
# URL: http://www.gnu.org/software/patch/patch.html
# Paketçi: milisarge
# Gerekler:

name=patch
version=2.7.5
release=1

source=(http://ftp.gnu.org/gnu/patch/patch-$version.tar.xz)

build()
{
cd patch-$version
./configure --prefix=/usr
make
make prefix=$PKG/usr \
mandir=$PKG/usr/share/man install
}
